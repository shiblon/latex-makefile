<?xml version="1.0" encoding="UTF-8" ?>
<Module>
  <ModulePrefs title="LaTeX Makefile Downloads" />
  <Content type="html">
    <![CDATA[
    <div id="content_div" style="font-size:20pt; padding:5px;"></div>
    <script type="text/javascript">
      function manifestReceived(result) {
        document.getElementById('content_div').innerHTML = result;
      }

      gadgets.util.registerOnLoadHandler(function () {
        gadgets.io.makeRequest("/hg/Downloads/MANIFEST.json", manifestReceived);
      });
    </script>
    ]]>
  </Content>
</Module>
